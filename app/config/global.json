{
    "name": "wilda",
    "version": "1.4",
    "mode": "dev",
    "version_date": "2025-01-29",
    "styles": "relative",
    "scripts": "relative",
    "images": "relative",
    "fonts": "relative",
    "icons": "relative",
    "canonical": false,
    "debug": {
        "enabled": true
    },
    "seo": {
        "enabled": true,
        "ext": true
    },
    "storage":
    {
        "media": {
            "cache": "media/filecache"
        },
        "keys": {
            "pathMap": "map",
            "chromium": {
                "summary": "chromium:summary",
                "running": "chromium:running",
                "lastrun": "chromium:lastrun",
                "links": "chromium:links"
            }
        },
        "redis":
        {
            "compression": true,
            "host": "127.0.0.1",
            "port": 6379
        }
    },
    "lang": "ru",
    "forceSSL": true,
    "compress":
    {
        "enabled": true
    },
    "editor":
    {
        "enabled": true
    },
    "inject":
    {
        "enabled": false,
        "header": false,
        "footer": false
    },
    "metrics":
    {
        "enabled": false,
        "ya": "",
        "ga": ""
    },
    "favicon":
    {
        "enabled": false
    },
    "mail":
    {
        "send_type": "sendmail",
        "smtp": {
            "auth": true,
            "host": "mailhost.ru",
            "port": "25",
            "username": "test@mail.ru",
            "password": "userpass"
        },
        "enabled": true,
        "debug": "server",
        "subject": "Заявка с сайта",
        "from": "info@",
        "name": "submission",
        "success": "Сообщение успешно отправлено!",
        "error": "Ошибка!",
        "to": [
            "example@mail.ru"
        ]
    },
    "privoxy":
    {
        "enabled": true,
        "host": "5.39.220.187",
        "port": 8118
    },
    "cache":
    {
        "disk": {
            "mimeTypes": [
                "image/png",
                "image/jpeg",
                "application/font-woff",
                "font/woff2",
                "application/octet-stream",
                "image/x-icon"
            ]
        },
        "enabled": true,
        "browser": true,
        "expire": 86400,
        "stats": true,
        "autocache": true,
        "cachettl": 6

    },
    "curl": {
      "timeout": 15,
      "encoding": "gzip"
    },
    "hosts": [
    {
        "type": "Tilda",
        "site": [
            "https://sitename.tld",
            "https://www.sitename.tld"
        ],
        "project": "https://project123.site.tld"
    }]
}